<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
   
    
    <title>{{allSetting.title}}</title>

    <link rel="shortcut icon" href="https://storage.googleapis.com/noxus-up-file/{{allSetting.ico}}">
    <meta name="description" content="{{allSetting.description}}">
    <meta name="keywords" content="{{allSetting.keywords}}"> 




    <link rel="stylesheet" type="text/css" href="qr/css/notify.css"/>
    <link rel="stylesheet" type="text/css" href="qr/css/last.css"/>
    <link href="qr/css/fontfamily.css" rel="stylesheet">
    <link href="qr/css/core.css" rel="stylesheet" />
    <link type="text/css" href="qr/css/custom.css" rel="stylesheet">
    <link type="text/css" href="qr/css/select2.css" rel="stylesheet">

</head>


<body>

            <nav class="tabbable sticky" style="padding: 0px 10px; top: 0px;"  >

                
                <ul class="nav nav-pills  mb-2" style="width: 90%;" id="qrmenu">

                    {{#each allCategoryQr}}
        
                    <li class="nav-item-category" style="margin-right: 10px; ">

                       <a class="nav-link mb-sm-3 mb-md-0 kategori" style="background-color: black ; color: white;"   data-filter=".{{this.seoUrl}}" id="{{this.seoUrl}}" >{{this.name}}</a>
                   
                    </li>

                     {{/each}}




                </ul>


                <center>
               <a class="nav-link mb-sm-3 mb-md-0 kategori"  style="position:absolute; background-color: black ; color: white; width:10%; right:0px; top:8px; height:46px; padding:7px; margin-right: 5px; " id="allcategory"  >·êÅ</a>
                 </center>                   

            </nav>


    <div style="margin-top: -20px;"></div>
    <div class="wrapper" data-role="page" id="pageone">
    <section class="section pt-lg-0" id="restaurant-content" style="padding-top: 0px">

        <div class="container container-restorant"> 

            
            
             {{#each allProductsQr}}

             <div class="menu-restaurant {{this.categorySeoUrl}}" >


            <!-- category head start -->
            
                <div  style="text-align: center;" class="ktbaslik">
                    <h1 style="color: black;">{{this.categoryName}}</h1><br />
                </div>

            <!-- category head end -->

            <!-- procd list start -->

                 <div class="row">

{{#each this.allProducts}}

                      <div class="col-xl-3 col-lg-6 col-md-6 col-sm-6 ">
                            <div class="strip">
            
                                {{#if this.picture}}  
                                        <img src="https://storage.googleapis.com/noxus-up-file/{{this.picture}}" width="400" height="400">
                              {{/if}}

                              <div style="margin-top: 10px;"></div>


                             <span style="display: block; float:right" class="res_mimimum urunfiyat">{{this.price}}</span>


                              <span class="res_title" style="display: block;">  
                                  <b class="urunad" style="display:block; width: 85%;">{{this.name}}</b>
                            </span>
                            
                                <span class="res_description urundetay" style="display: block; margin-top: 10px;">{{this.description}}</span>
                
                            </div>
                        </div>
{{/each}}
      
                </div>
</div>


{{/each}}


        <!-- procd list end -->

        </div>  
    </section>
  </div>



  {{!-- all category modal start --}}


          <div class="modal fade" id="openmodal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
		<div class="modal-dialog">
			<div class="modal-content">
				<div class="modal-header" >
					<h5 class="modal-title ktbaslik"  style="margin-left: 35%;" id="ktb">Kategoriler</h5>
                    <button type="button" class="close" data-dismiss="modal">
                        <span style="color:black" aria-hidden="true">&times;</span>
                    </button>
				</div>

				<div class="modal-body">

          <ul class="nav nav-pills">

                    {{#each allCategoryQr}}
        
                    <li class="nav-item-category" style="margin:0px auto; width:100%; text-align: center;">
                       <a class="nav-link mb-sm-3 mb-md-0 kategori" style="background-color: black ; color: white;"  data-filter=".{{this.seoUrl}}" >{{this.name}}</a>

                    </li>

                     {{/each}}



                </ul>
					
				</div>
			</div>
		</div>
	</div>
  
  {{!-- all category modal end --}}

   
    <script src="qr/js/jquery1113.js"></script>
    <script src="qr/js/jqmobile.js" ></script>
    <script src="qr/js/boots.js" type="text/javascript"></script>
    <script src="qr/js/scrol.js"></script>
    <script src="qr/js/order.js"></script>

    
